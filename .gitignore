# Ignore node_modules directory
node_modules/

# Ignore .env files (dotenv configuration files)
.env

# Ignore Jest cache and coverage files
# (These files and directories are generated by Jest during testing)
coverage/
.cache/

# Ignore logs and artifacts generated by testing
logs/
*.log
npm-debug.log*

# Ignore any build or compiled files
# (e.g., transpiled JavaScript, bundled assets)
build/
dist/
out/

# Ignore any IDE or editor-specific files
# (e.g., .idea directory for IntelliJ IDEA)
.idea/
.vscode/

# Ignore any user-specific configuration files
# (e.g., .eslintrc.local or .editorconfig.user)
*.local
